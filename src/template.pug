doctype html
html(lang="en")
  head
    block title
      title QEDA &mdash; a tool for creating libraries of electronic components
    link(rel='shortcut icon' href='/img/favicon.ico' type='image/x-icon' sizes='16x16 24x24 32x32 64x64 128x128 256x256 512x512')

  body
    #wrap
      #content
        nav
          .container-fluid

          .navbar-header
            a.navbar-brand(href='/'): img(alt='QEDA', src='/img/logo.svg' width=60, height=60, style='margin-top: -20px;')

            .navbar-header.visible-sm.visible-xs
              button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                span.sr-only Toggle navigation
                span.icon-bar
                span.icon-bar
                span.icon-bar

          .collapse.navbar-collapse
            a.navbar-brand(href='/') QEDA
            ul.nav.nav-pills.pull-right
              li: a(href='/doc/') Documentation
              li: a.ext(href='https://github.com/qeda' target='_blank') Source Code
              li.active: a(href='/donate/') Donate

        .content
          block content

      #push

    #footer-wrapper
      footer.footer
        .container
          .row
            .col-md-8
              p Copyright &copy; 2015&ndash;2016 QEDA Authors
              p Powered by&nbsp;
                a(href='http://noviga.com/' target='_blank'): img(src='/img/noviga.png' height=30)
            .col-md-1
              a.cap(href='/') Home
            .col-md-1
              a.cap(href='/doc/') Doc
            .col-md-1
              a.cap.ext(href='https://github.com/qeda' target='_blank') Source
            .col-md-1
              a.cap(href='/donate/') Donate

  link(href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css', rel='stylesheet', integrity='sha256-7s5uDGW3AHqw6xtJmNNtr+OBRJUlgkNJEo78P4b0yRw= sha512-nNo+yCHEyn0smMxSswnf/OnX6/KwJuZTlNZBjauKhTK0c+zT+q5JOCx0UFhXQ6rJR9jg6Es8gPuD2uZcYDLqSw==', crossorigin='anonymous')
  link(href='/css/qeda.css', rel='stylesheet')
